{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "hi-eva",
  "version": "0.0.17",
  "identifier": "com.hi-eva.app",
  "build": {
    "beforeDevCommand": "pnpm ui:dev",
    "devUrl": "http://localhost:8990",
    "beforeBuildCommand": "pnpm ui:build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "category": "Utility",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI4MDJCQkVEMDVCNTEzMjAKUldRZ0U3VUY3YnNDS09XdW84RVhtbnc3MHR1ZXYwZGpiZHBhM08vOGVFMDRsbXpoc3dUNmdpOGcK",
      "windows": {
        "installMode": "passive"
      },
      "endpoints": [
        "https://github.com/0x-jerry/hi-eva-docs/releases/latest/download/latest.json"
      ]
    }
  }
}